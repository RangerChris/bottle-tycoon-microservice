{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "ReverseProxy": {
    "Routes": {
      "gameservice": {
        "ClusterId": "gameservice",
        "Match": {
          "Path": "/api/gameservice/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/gameservice"
          }
        ]
      }
    },
    "Clusters": {
      "gameservice": {
        "Destinations": {
          "destination1": {
            "Address": "http://gameservice:80"
          }
        }
      }
    }
  },
  "ConnectionStrings": {
    "Redis": "redis:6379"
  },
  "RabbitMQ": {
    "Host": "rabbitmq",
    "Username": "guest",
    "Password": "guest"
  },
  "OpenTelemetry": {
    "Jaeger": {
      "Endpoint": "http://jaeger:14268/api/traces"
    }
  },
  "OTEL_EXPORTER_JAEGER_ENDPOINT": "http://jaeger:14268/api/traces",
  "OTEL_SERVICE_NAME": "ApiGateway",
  "OTEL_TRACES_EXPORTER": "jaeger"
}