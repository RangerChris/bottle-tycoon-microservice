{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Debug"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "gameservice": {
        "ClusterId": "gameservice",
        "Match": {
          "Path": "/api/gameservice/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/gameservice"
          }
        ]
      },
      "recyclerservice": {
        "ClusterId": "recyclerservice",
        "Match": {
          "Path": "/api/recyclerservice/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/recyclerservice"
          }
        ]
      },
      "truckservice": {
        "ClusterId": "truckservice",
        "Match": {
          "Path": "/api/truckservice/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/truckservice"
          }
        ]
      },
      "headquartersservice": {
        "ClusterId": "headquartersservice",
        "Match": {
          "Path": "/api/headquartersservice/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/headquartersservice"
          }
        ]
      },
      "recyclingplantservice": {
        "ClusterId": "recyclingplantservice",
        "Match": {
          "Path": "/api/recyclingplantservice/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/recyclingplantservice"
          }
        ]
      }
    },
    "Clusters": {
      "gameservice": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5001"
          }
        }
      },
      "recyclerservice": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5002"
          }
        }
      },
      "truckservice": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5003"
          }
        }
      },
      "headquartersservice": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5004"
          }
        }
      },
      "recyclingplantservice": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5005"
          }
        }
      }
    }
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "RabbitMQ": {
    "Host": "localhost",
    "Username": "guest",
    "Password": "guest"
  },
  "OTEL_EXPORTER_JAEGER_ENDPOINT": "http://localhost:14268/api/traces"
}